{
  "name": "media-processing-server",
  "version": "1.0.0",
  "main": "src/app-lambda.js",
  "description": "",
  "private": true,
  "scripts": {
    "compile_ts": "rm -rf ./dist/* && tsc && cp package.json pnpm-lock.yaml ./dist && cd dist/ && pnpm install --production",
    "compile_esbuild": "rm -rf ./dist/* && pnpm esbuild ./src/app-lambda.ts --target=node18 --platform=node --bundle --outfile=dist/src/app-lambda.js",
    "deploy": "pnpm compile_ts && pnpm zip && AWS_PROFILE=wer1 terraform -chdir='./infrastructure' workspace select dev && AWS_PROFILE=wer1 terraform -chdir='./infrastructure' apply",
    "dev": "nodemon ./src/app-local.ts",
    "start": "node ./src/app-lambda.ts",
    "dev:local": "dotenv -e .env -- doppler run -- nodemon ./src/app-local.ts --preserve-env",
    "typecheck": "tsc",
    "generate": "doppler run -- graphql-codegen",
    "lint": "eslint --max-warnings=0 './src/**/*.ts' ",
    "test": "jest",
    "zip": "rm -f ./dist/deploy.zip && cd dist && zip -r deploy.zip *"
  },
  "dependencies": {
    "@apollo/client": "^3.7.17",
    "@aws-sdk/client-s3": "^3.374.0",
    "@aws-sdk/credential-providers": "^3.370.0",
    "@bull-board/api": "^5.7.0",
    "@bull-board/koa": "^5.7.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@koa/bodyparser": "^5.0.0",
    "@koa/router": "^12.0.0",
    "@types/lodash": "^4.14.195",
    "axios": "^1.4.0",
    "bullmq": "^4.6.0",
    "cross-fetch": "^4.0.0",
    "form-data": "^4.0.0",
    "glob": "^10.3.3",
    "graphql": "^16.7.1",
    "ioredis": "^5.3.2",
    "koa": "^2.14.2",
    "koa-helmet": "^7.0.2",
    "koa-logger": "^3.2.1",
    "lodash": "^4.17.21",
    "pino": "^8.16.1",
    "pino-pretty": "^10.2.3",
    "serverless-http": "^3.2.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
